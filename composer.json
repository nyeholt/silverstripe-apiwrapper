{
    "name": "symbiote/silverstripe-apiwrapper",
    "description": "Provides an API wrapper around service style classes",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "webservers"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "~4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\ApiWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\ApiWrapper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        },
        "expose": [
          "client/script"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
